{
    "name": "Vivid Monokai",
    "colors": {
        "editor.background": "#2B2B2B",
        "editorGroupHeader.tabsBackground": "#212121",
        "editorLineNumber.foreground": "#646464",
        "editorLineNumber.activeForeground": "#C8C8C8",
        "editorGutter.background": "#293134",
        "sideBar.background": "#2B2B2B",
        "tab.activeBorder": "#1976D2",
        "tab.hoverBackground": "#424242",
        "tab.border": "#212121",
        "tab.activeBackground": "#424242",
        "tab.inactiveBackground": "#212121",
        "terminal.foreground" : "#16C60C",
        "editorCursor.foreground": "#BBBBBB",
        "editorIndentGuide.background": "#424242",
        "editorIndentGuide.activeBackground": "#757575",
        "editorBracketMatch.background": "#3A6DA0",
        "editorBracketMatch.border": "#3A6DA0",
        "editor.foreground": "#A9B7C6",
        "parameterHints.hintForeground": "#9E9E9E",
        "parameterHints.hintBackground": "#3C3F41",
    },
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "*.static": {
            "fontStyle": "italic"
        },
        "property.readonly": {
            "fontStyle": "",
            "foreground": "#66E1F8"
        }
    },
    "tokenColors": [
        {
            "name": "Text, braces and ponctuation",
            "scope": [
                "constant.other.color",
                "keyword.operator.optional",
                "keyword.operator.definiteassignment",
                "keyword.operator.assignment",
                "keyword.operator.type.annotation",
                "keyword.operator.logical",
                "keyword.operator.comparison",
                "meta.brace.round",
                "meta.brace.square",
                "meta.type.annotation",
                "punctuation.definition.typeparameters",
                "punctuation.terminator.statement",
                "punctuation.separator.parameter",
                "punctuation.separator.comma",
                "punctuation.definition.block",
                "punctuation.definition.template-expression",
                "punctuation.accessor",
                "punctuation.definition.parameters",
                "punctuation.separator.key-value",
            ],
            "settings": {
                "foreground": "#A9B7C6",
                "fontStyle": ""
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "keyword.control.import",
                "keyword.control.from",
                "keyword.control.as",
                "keyword.control.export",
                "keyword.control.flow",
                "keyword.control.conditional",
                "keyword.control.loop",
                "keyword.control.switch",
                "keyword.operator.expression.of",
                "keyword.operator.expression.in",
                "keyword.operator.expression.instanceof",
                "storage.type",
                "storage.type.class",
                "storage.type.type",
                "storage.modifier",
                "support.type.builtin",
                "support.type.primitive",
                "variable.language.this",
                "constant.language.boolean",
                "constant.language.null",
                "meta.arrow",
                "new.expr",
            ],
            "settings": {
                "foreground": "#DD2867",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Comments - Inline and Multiple",
            "scope": [
                "comment.line",
                "comment.block"
            ],
            "settings": {
                "foreground": "#757575",
                "fontStyle": ""
            }
        },
        {
            "name": "Comments - Documentation",
            "scope": [
                "comment.block.documentation"
            ],
            "settings": {
                "foreground": "#629755",
                "fontStyle": ""
            }
        },
        {
            "name": "Comments - Documentation - Tags",
            "scope": [
                "comment.block.documentation storage.type.class.jsdoc",
                "comment.block.documentation punctuation.definition.block.tag.jsdoc"
            ],
            "settings": {
                "foreground": "#629755",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Comments - Documentation - Tags - Value",
            "scope": [
                "comment.block.documentation variable.other.jsdoc",
            ],
            "settings": {
                "foreground": "#629755",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Imports",
            "scope": [
                "meta.import variable.other.readwrite.alias"
            ],
            "settings": {
                "foreground": "#A9B7C6",
                "fontStyle": ""
            }
        },
        {
            "name": "Parameters",
            "scope": [
                "variable.parameter",
            ],
            "settings": {
                "foreground": "#79ABFF",
                "fontStyle": ""
            }
        },
        {
            "name": "Local variables and constants",
            "scope": [
                "variable.other"
            ],
            "settings": {
                "foreground": "#ED7F48",
                "fontStyle": ""
            }
        },
        {
            "name": "Interfaces and Type Alias",
            "scope": [
                "entity.name.type.interface",
                "entity.name.type.alias"
            ],
            "settings": {
                "foreground": "#80F2F6",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type.class",
                "entity.name.type"
            ],
            "settings": {
                "foreground": "#1290C3",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Classes - Fields",
            "scope": [
                "variable.object.property",
                "variable.other.property",
                "variable.other.object.property"
            ],
            "settings": {
                "foreground": "#66E1F8",
                "fontStyle": ""
            }
        },
        {
            "name": "Classes - Methods",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#1EB540",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Decorators",
            "scope": [
                "meta.decorator entity.name.function",
                "punctuation.decorator"
            ],
            "settings": {
                "foreground": "#808000",
                "fontStyle": ""
            }
        },
        {
            "name": "Objects",
            "scope": [
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#66E1F8",
                "fontStyle": ""
            }
        },
        {
            "name": "Strings",
            "scope": [
                "string.quoted.double",
                "punctuation.definition.string",
                "string.template"
            ],
            "settings": {
                "foreground": "#6A8759",
                "fontStyle": ""
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#AE81FF",
                "fontStyle": ""
            }
        }
    ]
}