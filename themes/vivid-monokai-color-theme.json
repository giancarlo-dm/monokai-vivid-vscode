{
    "name": "Vivid Monokai",
    "colors": {
        "editor.background": "#2B2B2B",
        "editor.foldBackground": "#00000050",
        "editor.foreground": "#A9B7C6",
        "editorBracketMatch.background": "#3A6DA0",
        "editorBracketMatch.border": "#3A6DA0",
        "editorCursor.foreground": "#BBBBBB",
        "editorError.background": "#ff000033",
        "editorError.foreground": "#FF0000",
        "editorGroupHeader.tabsBackground": "#212121",
        "editorGutter.background": "#293134",
        "editorIndentGuide.activeBackground": "#757575",
        "editorIndentGuide.background": "#424242",
        "editorInfo.background": "#20999d33",
        "editorInfo.foreground": "#20999D",
        "editorLineNumber.activeForeground": "#C8C8C8",
        "editorLineNumber.foreground": "#646464",
        "editorUnnecessaryCode.border": "#616161",
        "editorUnnecessaryCode.opacity": "#000000c0",
        "editorWarning.background": "#f0f00033",
        "editorWarning.foreground": "#F0F000",
        "parameterHints.hintBackground": "#3C3F41",
        "parameterHints.hintForeground": "#9E9E9E",
        "sideBar.background": "#2B2B2B",
        "tab.activeBackground": "#424242",
        "tab.activeBorder": "#1976D2",
        "tab.border": "#212121",
        "tab.hoverBackground": "#424242",
        "tab.inactiveBackground": "#212121",
        "terminal.foreground" : "#16C60C",
    },
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "*.static": {
            "fontStyle": "italic"
        },
        "enumMember": {
            "foreground": "#66E1F8"
        },
        "property": {
            "foreground": "#66E1F8"
        },
        "decorator": {
            "foreground": "#808000",
            "fontStyle": "bold"
        },
        "namespace": {
            "foreground": "#AFB42B",
            "fontStyle": ""
        },
        "type": {
            "foreground": "#80F2F6",
            "fontStyle": "bold"
        },
        "variable.defaultLibrary": {
            "foreground": "#D500F9",
            "fontStyle": "bold"
        }
    },
    "tokenColors": [
        {
            "name": "Text, braces and ponctuation",
            "scope": [
                "source",
                "constant.other.color",
                "keyword.operator.arithmetic",
                "keyword.operator.assignment",
                "keyword.operator.comparison",
                "keyword.operator.definiteassignment",
                "keyword.operator.increment",
                "keyword.operator.logical",
                "keyword.operator.optional",
                "keyword.operator.relational",
                "keyword.operator.spread",
                "keyword.operator.type.annotation",
                "meta.brace.round",
                "meta.brace.square",
                "meta.type.annotation",
                "punctuation.accessor",
                "punctuation.definition.block",
                "punctuation.definition.parameters",
                "punctuation.definition.template-expression",
                "punctuation.definition.typeparameters",
                "punctuation.separator.comma",
                "punctuation.separator.key-value",
                "punctuation.separator.parameter",
                "punctuation.separator.label",
                "punctuation.terminator.statement"
            ],
            "settings": {
                "foreground": "#A9B7C6",
                "fontStyle": ""
            }
        },
        {
            "name": "Keywords",
            "scope": [
                "constant.language.boolean",
                "constant.language.null",
                "constant.language.undefined",
                "keyword.control.as",
                "keyword.control.conditional",
                "keyword.control.export",
                "keyword.control.flow",
                "keyword.control.from",
                "keyword.control.import",
                "keyword.control.loop",
                "keyword.control.switch",
                "keyword.control.trycatch",
                "keyword.operator.expression.import",
                "keyword.operator.expression.in",
                "keyword.operator.expression.instanceof",
                "keyword.operator.expression.keyof",
                "keyword.operator.expression.of",
                "keyword.operator.expression.typeof",
                "meta.arrow",
                "new.expr",
                "storage.modifier",
                "storage.type.class",
                "storage.type.type",
                "storage.type",
                "support.type.builtin",
                "support.type.primitive",
                "variable.language.super",
                "variable.language.this"
            ],
            "settings": {
                "foreground": "#DD2867",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Template expression",
            "scope": [
                "punctuation.definition.template-expression"
            ],
            "settings": {
                "foreground": "#DD2867",
                "fontStyle": ""
            }
        },
        {
            "name": "Labels",
            "scope": [
                "entity.name.label"
            ],
            "settings": {
                "foreground": "#A5C261",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Comments - Inline and Multiple",
            "scope": [
                "comment.block",
                "comment.line"
            ],
            "settings": {
                "foreground": "#757575",
                "fontStyle": ""
            }
        },
        {
            "name": "Comments - Documentation",
            "scope": [
                "comment.block.documentation"
            ],
            "settings": {
                "foreground": "#629755",
                "fontStyle": ""
            }
        },
        {
            "name": "Comments - Documentation - Tags",
            "scope": [
                "comment.block.documentation punctuation.definition.block.tag.jsdoc",
                "comment.block.documentation storage.type.class.jsdoc"
            ],
            "settings": {
                "foreground": "#629755",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Comments - Documentation - Tags - Value",
            "scope": [
                "comment.block.documentation variable.other.jsdoc",
            ],
            "settings": {
                "foreground": "#629755",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Imports",
            "scope": [
                "meta.import variable.other.readwrite.alias"
            ],
            "settings": {
                "foreground": "#A9B7C6",
                "fontStyle": ""
            }
        },
        {
            "name": "Parameters",
            "scope": [
                "variable.parameter",
            ],
            "settings": {
                "foreground": "#79ABFF",
                "fontStyle": ""
            }
        },
        {
            "name": "Local variables and constants",
            "scope": [
                "variable.other"
            ],
            "settings": {
                "foreground": "#ED7F48",
                "fontStyle": ""
            }
        },
        {
            "name": "Interfaces and Type Alias",
            "scope": [
                "entity.name.type.alias",
                "entity.name.type.interface"
            ],
            "settings": {
                "foreground": "#80F2F6",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Classes",
            "scope": [
                "entity.name.type.class",
                "entity.name.type"
            ],
            "settings": {
                "foreground": "#1290C3",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Classes - Fields",
            "scope": [
                "variable.object.property",
                "variable.other.object.property",
                "variable.other.property"
            ],
            "settings": {
                "foreground": "#66E1F8",
                "fontStyle": ""
            }
        },
        {
            "name": "Classes - Methods",
            "scope": [
                "entity.name.function"
            ],
            "settings": {
                "foreground": "#1EB540",
                "fontStyle": "bold"
            }
        },
        {
            "name": "Type Parameters (Generics)",
            "scope": [
                "entity.name.type.parameter"
            ],
            "settings": {
                "foreground": "#00796B",
                "fontStyle": ""
            }
        },
        {
            "name": "Decorators",
            "scope": [
                "punctuation.decorator",
                "meta.decorator entity.name.function"
            ],
            "settings": {
                "foreground": "#808000",
                "fontStyle": ""
            }
        },
        {
            "name": "Objects",
            "scope": [
                "meta.object-literal.key"
            ],
            "settings": {
                "foreground": "#66E1F8",
                "fontStyle": ""
            }
        },
        {
            "name": "Strings",
            "scope": [
                "punctuation.definition.string",
                "string.quoted.double",
                "string.quoted.single",
                "string.template"
            ],
            "settings": {
                "foreground": "#6A8759",
                "fontStyle": ""
            }
        },
        {
            "name": "Numbers",
            "scope": [
                "constant.numeric"
            ],
            "settings": {
                "foreground": "#AE81FF",
                "fontStyle": ""
            }
        },

        {
            "name": "HTML - Text and Punctuation",
            "scope": [
                "punctuation.definition.tag.begin.html",
                "punctuation.definition.tag.end.html"
            ],
            "settings": {
                "foreground": "#A9B7C6",
                "fontStyle": ""
            }
        },
        {
            "name": "HTML - Tag",
            "scope": [
                "entity.name.tag.html",
            ],
            "settings": {
                "foreground": "#E8BF6A",
                "fontStyle": "bold"
            }
        },
        {
            "name": "HTML - Tag - Attribute",
            "scope": [
                "entity.other.attribute-name.html"
            ],
            "settings": {
                "foreground": "#BABABA",
                "fontStyle": "bold"
            }
        },
        {
            "name": "HTML - Tag - Attribute - Value",
            "scope": [
                "punctuation.definition.string.begin.html",
                "punctuation.definition.string.end.html",
                "string.quoted.double.html"
            ],
            "settings": {
                "foreground": "#A5C261",
                "fontStyle": ""
            }
        },
        {
            "name": "HTML - Tag - Angular Binding",
            "scope": [
                "entity.other.ng-binding-name.event.html",
                "entity.other.ng-binding-name.property.html"
            ],
            "settings": {
                "foreground": "#66E1F8",
                "fontStyle": "bold"
            }
        },
        {
            "name": "HTML - Tag - Angular Binding - Template",
            "scope": [
                "entity.other.ng-binding-name.template.html"
            ],
            "settings": {
                "foreground": "#66E1F8",
                "fontStyle": "italic bold"
            }
        },
        {
            "name": "HTML - Entity Reference",
            "scope": [
                "constant.character.entity.named",
                "punctuation.definition.entity.html"
            ],
            "settings": {
                "foreground": "#AE81FF",
                "fontStyle": "bold"
            }
        },

        {
            "name": "CSS - Keywords",
            "scope": [
                "keyword.control.at-rule.font-face.css",
                "keyword.control.at-rule.keyframes.css",
                "keyword.control.at-rule.media.css",
                "keyword.control.at-rule.fontface"
            ],
            "settings": {
                "foreground": "#D62967",
                "fontStyle": "bold"
            }
        },
        {
            "name": "CSS - Class, Media and Keyframes",
            "scope": [
                "entity.other.attribute-name.class.css",
                "support.constant.media.css",
                "support.type.property-name.media.css",
                "support.type.vendored.property-name.media.css",
                "variable.parameter.keyframe-list.css"
            ],
            "settings": {
                "foreground": "#A6E22E",
                "fontStyle": ""
            }
        },
        {
            "name": "CSS - Property",
            "scope": [
                "support.type.property-name.css",
                "support.type.vendored.property-name.css"
            ],
            "settings": {
                "foreground": "#66D9EF",
                "fontStyle": "italic"
            }
        },
        {
            "name": "CSS - Property Value",
            "scope": [
                "keyword.other.unit.cm.css",
                "keyword.other.unit.em.css",
                "keyword.other.unit.mm.css",
                "keyword.other.unit.pt.css",
                "keyword.other.unit.px.css",
                "keyword.other.unit.rem.css",
                "keyword.other.unit.vh.css",
                "keyword.other.unit.vw.css",
                "meta.property-value.css",
                "support.constant.font-name.css",
                "support.constant.property-value.css"
            ],
            "settings": {
                "foreground": "#66D9EF",
                "fontStyle": "bold"
            }
        },
        {
            "name": "CSS - Tag and Selectors",
            "scope": [
                "entity.name.tag.css",
                "entity.name.tag.custom",
                "entity.name.tag.wildcard.css",
                "entity.other.keyframe-offset.css",
                "meta.selector.css"
            ],
            "settings": {
                "foreground": "#F92672",
                "fontStyle": ""
            }
        },
        {
            "name": "CSS - Attribute Name and Value",
            "scope": [
                "entity.other.attribute-name.attribute.css",
                "entity.other.attribute-name.css",
                "string.unquoted.attribute-value.css"
            ],
            "settings": {
                "foreground": "#A6E22E",
                "fontStyle": ""
            }
        },
        {
            "name": "CSS - HEX Color",
            "scope": [
                "constant.other.color.rgb-value.hex.css"
            ],
            "settings": {
                "foreground": "#A6E22E",
                "fontStyle": ""
            }
        },
        {
            "name": "CSS - Pseudo Selector",
            "scope": [
                "entity.other.attribute-name.pseudo-class.css",
                "entity.other.attribute-name.pseudo-element.css"
            ],
            "settings": {
                "foreground": "#A6E22E",
                "fontStyle": ""
            }
        },
        {
            "name": "SCSS - Keywords",
            "scope": [
                "keyword.control.at-rule.import.scss",
                "keyword.control.at-rule.include.scss",
                "keyword.control.at-rule.function.scss",
                "keyword.control.at-rule.mixin.scss",
                "keyword.control.at-rule.use.scss",
                "keyword.control.return.scss",
                "meta.at-rule keyword.control.operator"
            ],
            "settings": {
                "foreground": "#D62967",
                "fontStyle": "bold"
            }
        },
        {
            "name": "SCSS - Interpolation",
            "scope": [
                "punctuation.definition.interpolation.begin.bracket.curly.scss",
                "punctuation.definition.interpolation.end.bracket.curly.scss"
            ],
            "settings": {
                "foreground": "#D62967",
                "fontStyle": ""
            }
        },
        {
            "name": "SCSS - Attribute Name and Value",
            "scope": [
                "entity.other.attribute-name.attribute.scss",
                "string.unquoted.attribute-value.scss"
            ],
            "settings": {
                "foreground": "#A6E22E",
                "fontStyle": ""
            }
        },
        {
            "name": "SCSS - Variable",
            "scope": [
                "variable.scss"
            ],
            "settings": {
                "foreground": "#FD971F",
                "fontStyle": "italic"
            }
        },
        {
            "name": "SCSS - Function",
            "scope": [
                "entity.name.function.scss",
                "support.function.misc.scss",
            ],
            "settings": {
                "foreground": "#1EB540",
                "fontStyle": "bold"
            }
        },
        {
            "name": "SCSS - &",
            "scope": [
                "entity.name.tag.reference.scss"
            ],
            "settings": {
                "foreground": "#F92672",
                "fontStyle": ""
            }
        },

        {
            "name": "JSON - Keywords",
            "scope": [
                "constant.language.json"
            ],
            "settings": {
                "foreground": "#D62967",
                "fontStyle": "bold"
            }
        },
        {
            "name": "JSON - Properties",
            "scope": [
                "support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#66E1F8",
                "fontStyle": ""
            }
        }
    ]
}
